{
  "name": "@zkpassport/registry",
  "version": "0.1.11",
  "description": "Registry SDK for interacting with the ZKPassport Registry",
  "author": "ZKPassport",
  "license": "MIT",
  "type": "module",
  "types": "./dist/index.d.ts",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "files": [
    "dist"
  ],
  "keywords": [
    "zkpassport",
    "zkpassport registry",
    "certificate registry",
    "circuit registry"
  ],
  "scripts": {
    "test": "bun test tests/*.test.ts",
    "coverage": "bun test tests/*.test.ts --coverage",
    "check": "tsc --noEmit && prettier -c src tests && eslint src tests --ext .ts",
    "format": "prettier -c -w src tests",
    "build": "rimraf ./dist && bun run build:bundle && bun run build:types",
    "build:bundle": "node scripts/esbuild.mjs",
    "build:types": "node scripts/gen-types.mjs",
    "build:watch": "rimraf ./dist && concurrently \"node scripts/esbuild.mjs --watch\" \"node scripts/gen-types.mjs --watch\"",
    "prepublishOnly": "bun run format && bun run check && bun run test && bun run build"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "default": "./dist/esm/index.js"
    }
  },
  "dependencies": {
    "@zkpassport/utils": "^0.8.1",
    "debug": "^4.4.0"
  },
  "devDependencies": {
    "@types/bun": "^1.2.12",
    "@types/debug": "^4.1.12",
    "@types/jest": "^29.5.14",
    "@types/node": "^18.19.87",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "concurrently": "^8.2.2",
    "esbuild": "^0.25.3",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "keccak256": "^1.0.6",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}
