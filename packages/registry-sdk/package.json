{
  "name": "@zkpassport/registry",
  "version": "0.12.0",
  "description": "Registry SDK for interacting with the ZKPassport Registry",
  "author": "ZKPassport",
  "license": "Apache-2.0",
  "type": "module",
  "main": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup --config tsup.config.ts",
    "dev:build": "DEV_BUILD=true bun run build",
    "clean": "rimraf dist tsconfig.tsbuildinfo",
    "test": "bun test tests/*.test.ts",
    "coverage": "bun test tests/*.test.ts --coverage",
    "check": "prettier -c src tests && eslint src tests --ext .ts",
    "format": "prettier -c -w src tests",
    "validate-package": "../../scripts/validate-package.sh",
    "prepublishOnly": "../../scripts/prepublish.sh"
  },
  "files": [
    "dist/esm",
    "dist/cjs"
  ],
  "keywords": [
    "zkpassport",
    "registry",
    "zkpassport registry",
    "certificate registry",
    "circuit registry",
    "sanctions registry"
  ],
  "dependencies": {
    "@zkpassport/poseidon2": "^0.6.2",
    "@zkpassport/utils": "workspace:*",
    "debug": "^4.4.0",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@sinonjs/fake-timers": "^15.0.0",
    "keccak256": "^1.0.6"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "packageManager": "bun@1.3.1"
}
