{
  "name": "@zkpassport/sdk",
  "version": "0.5.6",
  "description": "Privacy-preserving identity verification using passports and ID cards",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "type": "module",
  "files": [
    "src/",
    "dist/**",
    "tsconfig.json",
    "README.md"
  ],
  "scripts": {
    "prepublishOnly": "bun run test && bun run build",
    "build": "tsc && tsc -p tsconfig.cjs.json && bun run build:fix-aliases",
    "build:esm": "tsc && bun run build:fix-aliases",
    "build:cjs": "tsc -p tsconfig.cjs.json && bun run build:fix-aliases",
    "build:fix-aliases": "bun run scripts/fix-aliases.mjs",
    "test": "bun test tests/*.test.ts",
    "test:debug": "bun test tests/*.test.ts --inspect-wait",
    "deploy": "npm publish --access public"
  },
  "keywords": [],
  "author": "",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.25.9",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-typescript": "^7.26.0",
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.9",
    "@types/pako": "^2.0.3",
    "@types/ws": "^8.5.12",
    "jest": "^29.7.0",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "@aztec/bb.js": "^0.82.2",
    "@noble/ciphers": "^1.2.1",
    "@noble/hashes": "^1.7.2",
    "@noble/secp256k1": "^2.2.3",
    "@obsidion/bridge": "^0.10.2",
    "@zkpassport/registry": "workspace:*",
    "@zkpassport/utils": "workspace:*",
    "buffer": "^6.0.3",
    "i18n-iso-countries": "^7.12.0",
    "pako": "^2.1.0",
    "viem": "^2.27.2",
    "ws": "^8.18.0"
  },
  "packageManager": "bun@1.2.13"
}
