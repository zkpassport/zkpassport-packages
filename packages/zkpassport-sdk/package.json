{
  "name": "@zkpassport/sdk",
  "version": "0.12.1",
  "description": "Privacy-preserving identity verification using passports and ID cards",
  "author": "ZKPassport",
  "license": "Apache-2.0",
  "type": "module",
  "main": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.cjs"
    }
  },
  "scripts": {
    "build": "bun run build:types && tsup --config tsup.config.ts",
    "build:types": "tsc -b && tsc-alias -v -p tsconfig.json -f",
    "dev:build": "DEV_BUILD=true bun run build",
    "clean": "rimraf dist tsconfig.tsbuildinfo",
    "test": "bun test",
    "coverage": "bun test --coverage",
    "check": "prettier -c src tests && eslint src tests --ext .ts",
    "format": "prettier -c -w src tests",
    "validate-package": "../../scripts/validate-package.sh",
    "prepublishOnly": "../../scripts/prepublish.sh"
  },
  "files": [
    "dist/esm",
    "dist/cjs",
    "dist/types"
  ],
  "keywords": [
    "zkpassport",
    "zkpassport-sdk",
    "sdk"
  ],
  "dependencies": {
    "@aztec/bb.js": "2.0.3",
    "@noble/ciphers": "^1.2.1",
    "@noble/hashes": "^1.7.2",
    "@noble/secp256k1": "^2.2.3",
    "@obsidion/bridge": "^0.11.1",
    "@zkpassport/registry": "workspace:*",
    "@zkpassport/utils": "workspace:*",
    "buffer": "^6.0.3",
    "i18n-iso-countries": "^7.12.0",
    "pako": "^2.1.0",
    "tslib": "^2.8.1",
    "viem": "^2.27.2",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/pako": "^2.0.3",
    "@types/ws": "^8.5.12"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "packageManager": "bun@1.3.1"
}
